      "hireable": true
    }
  ],
  "scripts": {
    "push": "clear && GIT_BRANCH=$(git symbolic-ref HEAD | sed 's/.*\\///' | tr '_' ' ') && GIT_USER=$(git config user.name) && clear && git pull origin main && git add . && read -p \"\n${GIT_USER}, ¿Qué cambios o ajustes hiciste en esta versión? \n ¡Cuéntanos todo! : \" CUSTOM_MESSAGE && git commit -m \"$(date +\"%d%b%H%M\") ⛘ ${CUSTOM_MESSAGE} - ${GIT_BRANCH}\" && git push",
    "lint": "eslint .",
    "serve": "firebase emulators:start --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
      "node": "16"
  },
  "main": "index.js",
  "dependencies": {
    "archiver": "^5.3.1",
    "cors": "^2.8.5",
    "deepmerge": "^4.2.2",
    "firebase-admin": "^11.8.0",
    "firebase-functions": "^4.3.1",
    "google-libphonenumber": "^3.2.27",
    "googleapis": "^108.0.0",
    "memory-cache": "^0.2.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.35",
    "node-fetch": "^2.6.1",
    "xmlbuilder2": "^3.0.2",
    "xmldom": "^0.6.0",
    "xpath": "^0.0.32"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "eslint": "^8.15.0",
    "eslint-config-google": "^0.14.0",
    "firebase-functions-test": "^3.1.0",
    "mocha": "^10.2.0",
    "sinon": "^15.1.0"
  },
  "private": true
}
